{
  "name": "mockup-compiler-js",
  "version": "1.1.2",
  "description": "Tool to convert Excel unit test data to text and archive it",
  "type": "module",
  "bin": {
    "mockup-compiler": "./src/index.js"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest NODE_NO_WARNINGS=1 --watch",
    "testrun": "node index.js -c test-sample/.mock-config.json",
    "lint": "eslint index.js lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbcgua/mockup-compiler-js.git"
  },
  "private": false,
  "author": "Alexander Tsybulsky",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "lodash-es": "^4.17.21",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "deep-freeze": "0.0.1",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "memfs": "^4.17.0",
    "vitest": "^3.0.4"
  }
}
